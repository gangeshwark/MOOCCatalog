<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme.Theme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryColorDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlHighlight">@color/textIcons</item>
        <!--    <item name="android:windowNoTitle">true</item>
            <item name="windowActionBar">false</item>-->
        <item name="windowActionModeOverlay">true</item>
        <item name="divider">@color/dividerColor</item>
        <item name="android:textColorPrimary">@color/primaryText</item>
        <item name="android:textColorSecondary">@color/secondaryText</item>

        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="theme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
    </style>

    <style name="AppTheme.Tunefit" parent="AppTheme.Theme">

    </style>

    <style name="AppTheme.Tunefit.WithoutActionBar">
        <!-- Both of these are needed -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>



</resources>